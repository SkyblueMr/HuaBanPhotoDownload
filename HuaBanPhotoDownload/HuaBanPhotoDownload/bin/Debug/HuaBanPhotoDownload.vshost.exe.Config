<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!--是否在debug-->
    <add key="Debug" value="false"/>

    <!--存放文件夹-->
    <add key="ImageDir" value="Images"/>

    <!--重试次数-->
    <add key="MaxTryTimes" value="5"/>

    <!--线程数量-->
    <add key="ThreadCount" value="5"/>
  </appSettings>
</configuration>